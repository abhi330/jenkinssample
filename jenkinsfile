pipeline {
    agent any

    tools{
        maven "MAVEN_HOME"
    }
    stages {
        stage("Checkout code"){
            steps
            {
                checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/abhi330/jenkinssample.git']])
                echo 'Check Out'
            }
        }
        stage('Clean') {
            steps {
                sh 'mvn -f pom.xml clean install'
                echo 'Cleaning..' }}}}
